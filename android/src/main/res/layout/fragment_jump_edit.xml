<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/EditJumpContainer"
    tools:context="org.tomcurran.remiges.ui.JumpEditFragment">

    <EditText
        android:id="@+id/edit_jump_number"
        android:inputType="number"
        style="@style/EditJump.Number" />

    <TextView
        android:id="@+id/edit_jump_date"
        style="@style/EditJump.Date" />

    <EditText
        android:id="@+id/edit_jump_description"
        android:inputType="textMultiLine"
        style="@style/EditJump.Description" />

</RelativeLayout>
